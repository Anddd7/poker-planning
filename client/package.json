{
  "name": "poker-planning",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "checkTs": "tsc --noEmit",
    "format": "prettier '**/*.{css,json,js,ts,tsx,md,mdx}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --fix",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.4.15",
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@mui/icons-material": "5.0.1",
    "@mui/lab": "5.0.0-alpha.49",
    "@mui/material": "5.0.2",
    "@mui/system": "5.0.2",
    "graphql": "15.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web-vitals": "2.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/near-operation-file-preset": "2.1.4",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.1.4",
    "@graphql-codegen/typescript-operations": "2.1.5",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.1",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.10.2",
    "@types/react": "17.0.26",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.5.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.12.4",
    "prettier": "2.4.1",
    "react-scripts": "4.0.3",
    "typescript": "4.4.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
